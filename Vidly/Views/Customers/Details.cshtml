@model Vidly.Models.Customer
@{
    ViewData["Title"] = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>@Model.MembershipType.Name</li>
    @if (Model.Birthdate != null)
    {
        <li>
            @Html.DisplayFor(m => m.Birthdate)
        </li>
    }
</ul>
